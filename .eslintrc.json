{
  "extends": "standard",
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
    "use": true
  },
  "rules": {
    "space-unary-ops": "off",
    "no-multi-spaces": "off",
    "key-spacing": "off",
    "padded-blocks": "off",
    "no-unused-vars": [
      "error",
      { "vars": "all" }
    ]
  }
}
